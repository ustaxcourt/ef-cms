{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": ["./src/", "../shared/src/", "../web-api/src/"],
    "includePattern": ".+\\.js$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": { "recurse": true, "destination": "./dist/jsdoc/" },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
