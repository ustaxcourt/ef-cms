  "scripts": {
    "build:all": "npm run clean && npm run build:assets && npm run build:_api -- web-api/src/apiHandlers.js web-api/src/casesHandlers.js web-api/src/usersHandlers.js web-api/src/caseDeadlinesHandlers.js web-api/src/caseDocumentsHandlers.js web-api/src/caseNotesHandlers.js web-api/src/documentsHandlers.js web-api/src/workItemsHandlers.js web-api/src/sectionsHandlers.js web-api/src/trialSessionsHandlers.js web-api/src/notificationHandlers.js web-api/src/streamsHandlers.js && mkdir -p web-api/dist && find dist -name '*Handlers.js' -exec cp {} web-api/dist/ \\; && npm run build:client",
    "build:api:cases": "npm run build:_api -- web-api/src/casesHandlers.js",
    "build:api:case:documents": "npm run build:_api -- web-api/src/caseDocumentsHandlers.js",
    "build:api:case:deadlines": "npm run build:_api -- web-api/src/caseDeadlinesHandlers.js",
    "build:api:case:notes": "npm run build:_api -- web-api/src/caseNotesHandlers.js",
    "build:api:documents": "npm run build:_api -- web-api/src/documentsHandlers.js",
    "build:api:notifications": "npm run build:_api -- web-api/src/notificationHandlers.js",
    "build:api:sections": "npm run build:_api -- web-api/src/sectionsHandlers.js",
    "build:api:streams": "npm run build:_api -- web-api/src/streamsHandlers.js",
    "build:api:trialSessions": "npm run build:_api -- web-api/src/trialSessionsHandlers.js",
    "build:api:users": "npm run build:_api -- web-api/src/usersHandlers.js",
    "build:api:workItems": "npm run build:_api -- web-api/src/workItemsHandlers.js",
    "build:api:public": "npm run build:_api -- web-api/src/publicApiHandlers.js",
    "build:api": "npm run build:_api -- web-api/src/apiHandlers.js",
    "build:_api": "parcel build --no-source-maps --bundle-node-modules --target node",
    "build:assets": "node shared/createModule.js",
    "build:client": "USTC_ENV=prod parcel build --no-source-maps web-client/src/index.pug; npm run build:jsdoc",
    "build:public": "USTC_ENV=prod parcel build --no-source-maps -d dist-public web-client/src/index-public.pug -o index.html; npm run build:jsdoc",
    "build:jsdoc": "jsdoc -c web-client/jsdoc.conf.json",
    "clean": "rm -rf dist/ web-client/.cache/",
    "clean:public": "rm -rf dist-public/",
    "cypress:open": "cypress open --config watchForFileChanges=true",
    "cypress": "cypress run",
    "cypress:smoketests": "cypress run -C cypress-smoketests.json --env AWS_ACCESS_KEY_ID=$AWS_ACCESS_KEY_ID,AWS_SECRET_ACCESS_KEY=$AWS_SECRET_ACCESS_KEY,ENV=$ENV",
    "cypress:smoketests:open": "cypress open -C cypress-smoketests.json --env AWS_ACCESS_KEY_ID=$AWS_ACCESS_KEY_ID,AWS_SECRET_ACCESS_KEY=$AWS_SECRET_ACCESS_KEY,ENV=$ENV",
    "dynamo:admin": "DYNAMO_ENDPOINT=http://localhost:8000 dynamodb-admin",
    "dynamo:export": "AWS_ACCESS_KEY_ID=noop AWS_SECRET_ACCESS_KEY=noop SLS_DEPLOYMENT_BUCKET=noop node web-api/dynamo-export.js > ./web-api/storage/fixtures/efcms.json",
    "dynamo:import": "pushd ./web-api && node ./create-dynamo-tables.js && node ./seed-dynamo.js ./storage/fixtures/efcms.json && popd",
    "lint:swagger": "swagger-cli validate web-api/swagger.json",
    "lint:css:fix": "npm run lint:css -- --fix",
    "lint:css": "stylelint web-client/src/**/*.scss",
    "lint:fix": "run-p lint:css:fix lint:js:fix",
    "lint:js": "TIMING=1 eslint --ext .js,.jsx shared/src/ web-api/src/ web-client/src/",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:js:fix:plain": "npm run lint:js:fix -- --rule 'prettier/prettier: 0'",
    "lint": "npx run-p lint:swagger lint:css lint:js",
    "print:success": "echo 'Build successful. You can access the site at http://localhost:1234'",
    "seed:db": "node ./web-api/create-dynamo-tables.js && node ./web-api/seed-dynamo.js",
    "seed:s3": "export S3_DIR=./web-api/storage/s3 && rm -rf \"${S3_DIR}\" && cp -R ./web-api/storage/fixtures/s3/ \"${S3_DIR}\"",
    "seed": "run-p seed:db seed:s3",
    "seed:elasticsearch": "./web-api/seed-elasticsearch.sh",
    "start:api": "./run-local.sh",
    "start:api:resume": "RESUME=true npm run start:api",
    "start:api:ci": "CI=true npm run start:api",
    "start:madge": "node ./graph-generators/index.js",
    "start:client": "npm run clean && FILE_UPLOAD_MODAL_TIMEOUT=1 USTC_ENV=dev parcel --no-cache --no-source-maps web-client/src/index.pug",
    "start:client:ci": "CI=true SKIP_VIRUS_SCAN=true npm run start:client",
    "start:client:no-scanner": "NO_SCANNER=true npm run start:client",
    "start:client:debug": "USTC_DEBUG=true npm run start:client",
    "start:public": "npm run clean:public && USTC_ENV=dev parcel --port 5678 -d dist-public --no-cache --no-source-maps web-client/src/index-public.pug",
    "test": "echo 'Best run with start:api:ci and start:client:ci' && sleep 3 && CI=true npx run-s test:shared test:api test:client test:pa11y cypress lint",
    "test:api": "jest --maxWorkers=4 --config web-api/jest.config.js \"web-api/.*\\.test\\.js\"",
    "test:_client": "NO_SCANNER=true SKIP_VIRUS_SCAN=true AWS_ACCESS_KEY_ID=noop AWS_SECRET_ACCESS_KEY=noop FILE_UPLOAD_MODAL_TIMEOUT=1 jest --runInBand --config web-client/jest.config.js",
    "test:client": "CI=false npm run test:_client -- \"web-client/.*\\.test\\.js\"",
    "test:client:integration": "npm run test:_client -- \"web-client/integration-tests/.*\\.test\\.js\"",
    "test:public:integration": "npm run test:_client -- \"web-client/integration-tests-public/.*\\.test\\.js\"",
    "test:client:unit": "npm run test:_client -- \"web-client/src/.*\\.test\\.js\"",
    "test:pa11y": "pa11y-ci --config web-client/pa11y-ci.config.js",
    "test:pa11y:public": "pa11y-ci --config web-client/pa11y-ci-public.config.js",
    "test:pa11y:screenshots": "cp -R web-client/pa11y/pa11y-screenshots/new/ web-client/pa11y/pa11y-screenshots/old/ && pa11y-ci --config web-client/pa11y-ci-screenshots.config.js && rm -rf web-client/pa11y/pa11y-screenshots/diffs/* && node web-client/pa11y/compare-screenshots.js",
    "test:shared": "npm run build:assets && jest --maxWorkers=4 --config shared/jest.config.js \"shared/.*\\.test\\.js\"",
    "test:watch": "jest --watch .*test\\.js$",
    "watch": "nodemon -e yml --exec sls offline start"
  },
  "staticFiles": {
    "staticPath": "node_modules/pdfjs-dist/build",
    "watcherGlob": false
  }
}
